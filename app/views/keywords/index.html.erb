<p style="color: green"><%= notice %></p>
<div id="keywords">
  <div class="row">
      <div class="col-md-3 bg-light  fw-bold p-3">Keyword</div>
      <div class="col-md-3 bg-light  sfw-bold p-3">Action</div>
  </div>
  <% @keywords.each do |keyword| %>
    <div class="row">
      <div class="col-md-3"><%= keyword.name %></div>
      <div class="col-md-3"><%= link_to "Show Keyword", keyword %></div>
    </div>
  <% end %>
</div>
<nav aria-label="Page navigation">
  <ul class="pagination justify-content-center">
    <%= will_paginate @keywords, class: 'pagination' %>
  </ul>
</nav>
<br/>
<%= link_to "New keyword", new_keyword_path %>
